{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"kite.showWelcomeNotificationOnStartup": false,
	"explorer.confirmDelete": false,
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": true,
	"workbench.iconTheme": "minimal-icons",
	"editor.fontFamily": "MonoLisa",
	"terminal.integrated.fontFamily": "MonoLisa",
	"editor.fontLigatures": "'calt' off, 'liga' on, 'ss02' off",
	"editor.fontWeight": "200",
	"editor.lineHeight": 1.6,
	"editor.fontSize": 13.5,
	"editor.smoothScrolling": true,
	"window.menuBarVisibility": "compact",
	"window.titleSeparator": " ~ ",
	"outline.showStrings": false,
	"editor.guides.bracketPairs": "active",
	"html.format.wrapLineLength": 80,
	"html.format.wrapAttributes": "force-expand-multiline",
	"prettier.printWidth": 80,
	"prettier.useTabs": true,
	"window.title": "${activeEditorShort}  ${rootName}",
	"redhat.telemetry.enabled": false,
	"solidity.compileUsingRemoteVersion": "latest",
	"prettier.enableDebugLogs": true,
	"editor.inlineSuggest.enabled": true,
	"explorer.confirmDragAndDrop": false,
	"editor.bracketPairColorization.enabled": true,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"glassit.alpha": 245,
	"terminal.integrated.gpuAcceleration": "on",
	"workbench.colorCustomizations": {
		"[Minimal]": {
			"terminal.ansiBrightGreen": "#7c8151",
			"activityBar.background": "#1e1e1e"
		}
	},
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"workbench.productIconTheme": "icons-carbon",
	"editor.cursorStyle": "line-thin",
	"workbench.tree.expandMode": "doubleClick",
	"workbench.editor.autoLockGroups": {},
	"workbench.colorTheme": "Minimal",
	"window.zoomLevel": -1,
}
